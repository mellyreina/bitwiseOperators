#include <iostream>
#include <conio.h>
#include <stdio.h>
using namespace std;

int main()
{ 
   int a=150, b=165, c;
  
   c= a&b; /*(bitwise AND) in C or C++ takes two numbers as operands
    and does AND on every bit of two numbers. The result of AND is 1 only if both bits are 1.*/  
   printf("%d \n",c); 
   
   c=a|b; /* (bitwise OR) in C or C++ takes two numbers as operands and
    does OR on every bit of two numbers. The result of OR is 1 if any of the two bits is 1.*/
   printf("%d \n",c);
   
   c=a^b; /*(bitwise XOR) in C or C++ takes two numbers as operands 
   and does XOR on every bit of two numbers. The result of XOR is 1 if the two bits are different.*/
   printf("%d \n",c);
   
	getch();
	return 0;
}
